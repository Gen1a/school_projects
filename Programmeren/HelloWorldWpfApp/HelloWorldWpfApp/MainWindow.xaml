<Window x:Class="HelloWorldWpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorldWpfApp"
        mc:Ignorable="d"
        Title="Welkom" Height="250" Width="500">
    <Grid>
        <Button x:Name="OpenGroetWindowButton" Content="Open Groet Applicatie" HorizontalAlignment="Center" Height="30" Margin="0,50,0,0" VerticalAlignment="Top" Width="150" Click="OpenGroetWindowButton_Click"/>
        <Button x:Name="OpenCalendarWindowButton" Content="Open Calendar Applicatie" HorizontalAlignment="Center" Height="30" Margin="0,100,0,0" VerticalAlignment="Top" Width="150" Click="OpenCalendarWindowButton_Click"/>
        <Button x:Name="OpenKladblokWindowButton" Content="Open Kladblok Applicatie" HorizontalAlignment="Center" Height="30" Margin="0,150,0,0" VerticalAlignment="Top" Width="150" Click="OpenKladblokWindowButton_Click"/>
    </Grid>
</Window>
